{
  "taglib-imports": [
    "./elements/marko.json",
    "./blocks/marko.json"
  ],
  "<common-document>": {
    "template": "./document.marko",
    "<head>": {},
    "@newsletter": "object",
    "@date": "date"
  },
  "<common-static-styles>": {
    "template": "./static-styles.marko"
  },
  "<common-newsletter-data>": {
    "template": "./newsletter-data.marko"
  },
  "<common-table>": {
    "template": "./core-table.marko",
    "@border": "number",
    "@width": "string",
    "@class": "string",
    "@spacing": "number",
    "@padding": "number",
    "@align": "string",
    "@style": "string",
    "@attrs": "object"
  }
}
